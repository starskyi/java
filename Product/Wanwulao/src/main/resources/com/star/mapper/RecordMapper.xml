        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.star.mapper.RecordMapper">

    <insert id="add" useGeneratedKeys="true" keyProperty="id">
        insert into tb_record values(#{uid}, #{cid}, #{status}, #{price})
    </insert>

</mapper>